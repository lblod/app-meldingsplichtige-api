@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <https://www.w3.org/2000/01/rdf-schema#> .
@prefix cogs: <http://vocab.deri.ie/cogs#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix job:  <http://lblod.data.gift/jobs/> .
@prefix mu:   <http://mu.semte.ch/vocabularies/core/> .

# This file aims to include some static individuals from cogs and some useful extensions that are needed for this project.

job:Status
  a rdfs:Class ;
  rdfs:label "Status" ;
  rdfs:comment "Class used to add to statusses defined as classes in COGS to make the usable through mu-cl-resources." .

job:Operation
  a rdfs:Class ;
  rdfs:label "Operation" ;
  rdfs:comment "Class used to add to operations and transformation classes in COGS to make them usable through mu-cl-resources." .

# As defined in COGS

cogs:ExecutionStatus
  a rdfs:Class,
    owl:Class ;
  rdfs:label "ExecutionStatus" ;
  rdfs:comment "Provides the current execution status of a running process instance (a job). e.g. : fail, running, success. @en-us" .

# As defined in COGS with added job:Status as a class and uuids

cogs:Running
  a rdfs:Class,
    owl:Class ,
    job:Status ;
  mu:uuid "097628ce-4268-4c0b-9b7c-ffe5e9b1e47f" ;
  rdfs:label "Running" ;
  rdfs:comment "Status of a job that is in the state of being operated. @en-us" ;
  rdfs:subClassOf cogs:ExecutionStatus .

cogs:Fail
  a rdfs:Class,
    owl:Class ,
    job:Status ;
  mu:uuid "c4c5655c-ff01-4580-bf8e-d039c992784a" ;
  rdfs:label "Fail" ;
  rdfs:comment "Status of a job that stops operating or functioning. @en-us" ;
  rdfs:subClassOf cogs:ExecutionStatus .

cogs:Success
  a rdfs:Class,
    owl:Class ,
    job:Status ;
  mu:uuid "513aa482-cf9b-447b-bd58-9115c5ad1414" ;
  rdfs:label "Success" ;
  rdfs:comment "Status of a job that had successful  performance. @en-us" ;
  rdfs:subClassOf cogs:ExecutionStatus .

#This is an addition to COGS! This is not original!
cogs:Pending
  a rdfs:Class,
    owl:Class ,
    job:Status ;
  mu:uuid "68e39a3c-cf33-4bd1-ac1d-00cfe274f582" ;
  rdfs:label "Pending" ;
  rdfs:comment "Status of a job that is pending. It might be waiting for another job to finish. @en-us" ;
  rdfs:subClassOf cogs:ExecutionStatus .

# As defined in COGS about transformations and operations (there is more than this in COGS) and added job:Operation for mu-cl-resources with uuids

cogs:Lookup
