cogs:AutomatedValidation
  a rdfs:Class ,
    owl:Class ,
    job:Operation ;
  mu:uuid "3fe72330-c304-4369-93aa-2a00b18651d7" ;
  rdfs:label "AutomatedValidation" ;
  rdfs:comment "Validation performed by a computer. @en-us" ;
  rdfs:subClassOf cogs:Validation .

