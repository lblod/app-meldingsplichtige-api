@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <https://www.w3.org/2000/01/rdf-schema#> .
@prefix mu:       <http://mu.semte.ch/vocabularies/core/> .
@prefix sch:      <http://www.schema.org/> .
@prefix services: <http://lblod.data.gift/services/> .

services:Service
  a rdfs:Class ;
  rdfs:label "Service" ;
  rdfs:comment "Defines a mu semtech service." .

services:automatic-submission-service
  a services:Service ;
  mu:uuid "f7d0ba9b-46d3-4f49-a74c-b1ffed0f706b" ;
