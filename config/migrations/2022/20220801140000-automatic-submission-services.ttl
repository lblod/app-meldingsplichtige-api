  rdfs:label "Automatic submission service" ;
  rdfs:comment "Microservice providing an API for external parties to submit publications to be processed automatically." .

services:download-url-service
  a services:Service ;
  mu:uuid "fc11bd33-0e42-4a56-a539-409f2d47de35" ;
  sch:codeRepository "https://github.com/lblod/download-url-service" ;
  rdfs:label "Download URL service" ;
  rdfs:comment "Microservice downloading a local copy of a remote file by URL." .

services:import-submission-service
  a services:Service ;
  mu:uuid "3c86255f-1d5b-407b-b2c6-58cd2099afb2" ;
  sch:codeRepository "https://github.com/lblod/import-submission-service" ;
  rdfs:label "Import submission service" ;
  rdfs:comment "Microservice to import knowledge about a submission harvested from a published document." .

services:enrich-submission-service
  a services:Service ;
  mu:uuid "6a3ed8e1-7730-4dcd-b05d-a42f9d2e0cd7" ;
  sch:codeRepository "https://github.com/lblod/enrich-submission-service" ;
  rdfs:label "Enrich submission service" ;
