@prefix mu:   <http://mu.semte.ch/vocabularies/core/> .

# This file aims to include some static individuals from cogs and some useful extensions that are needed for this project.

task:cogsOperation
  a rdf:Property ;
  rdfs:label "COGS operation" ;
  rdfs:comment "Links a task to an operation as described in COGS" ;
  rdfs:domain task:Task ;
  rdfs:range cogs:ExecutionStatus .

# As defined in COGS about transformations and operations (there is more than this in COGS) and added task:Operation for mu-cl-resources with uuids

cogs:Lookup
  a rdfs:Class ,
    owl:Class ,
    task:Operation ;
  mu:uuid "5ff3f55e-43a1-4a58-8b0f-f35777567759" ;
  rdfs:label "Lookup" ;
  rdfs:comment "A process used to look up data in a relational table, view or dictionary. @en-us" ;
  rdfs:subClassOf cogs:Transformation .

